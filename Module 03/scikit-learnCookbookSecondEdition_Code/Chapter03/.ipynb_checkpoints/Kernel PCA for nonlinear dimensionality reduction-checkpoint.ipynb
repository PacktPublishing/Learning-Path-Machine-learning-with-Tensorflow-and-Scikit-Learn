   "source": [
    "from sklearn import datasets, decomposition\n",
    "iris = datasets.load_iris()\n",
    "iris_X = iris.data\n",
    "\n",
    "\n",
    "kpca = decomposition.KernelPCA(kernel='cosine', n_components=2)\n",
    "iris_X_prime = kpca.fit_transform(iris_X)"
   ]
  }
